{
  "version": 2,
  "builds": [
    {
      "src": "myapp/package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "dist/myapp/browser",
        "installCommand": "cd myapp && npm install --legacy-peer-deps",
        "buildCommand": "cd myapp && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
